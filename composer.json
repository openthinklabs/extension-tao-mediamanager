{
    "name": "openthinklabs/extension-tao-mediamanager",
    "description": "extension to manage media in TAO",
    "support": {
        "forum": "http://forum.taotesting.com",
        "issues": "http://forge.taotesting.com"
    },
    "type": "tao-extension",
    "authors": [
        {
            "name": "Open Assessment Technologies"
        }
    ],
    "keywords": [
        "tao",
        "computer-based-assessment"
    ],
    "homepage": "http://www.taotesting.com",
    "license": [
        "GPL-2.0-only"
    ],
    "extra": {
        "tao-extension-name": "taoMediaManager"
    },
    "minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/oatbox-extension-installer.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/lib-generis-search.git"
		},        
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/generis.git"
		},                
		{
		    "type": "vcs",
		    "url": "git@github.com:openthinklabs/tao-core.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-item.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-itemqti.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-test.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-testqti-previewer.git"
		}        
	],		    
    "require": {
        "openthinklabs/oatbox-extension-installer": "~1.1||dev-master",
        "openthinklabs/lib-generis-search": "2.1.2",
        "openthinklabs/generis": ">=14.0.0",
        "openthinklabs/tao-core": ">=48.60.0",
        "openthinklabs/extension-tao-item": ">=11.13.4",
        "openthinklabs/extension-tao-itemqti": ">=28.22.0",
        "openthinklabs/extension-tao-test": ">=15.0.0",
        "openthinklabs/extension-tao-testqti-previewer": "*"
    },
    "autoload": {
        "psr-4": {
            "oat\\taoMediaManager\\": ""
        }
    }
}
